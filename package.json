{
  "name": "up-mcp",
  "version": "0.3.0",
  "description": "An unofficial MCP server for the Up Banking API.",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "up-banking",
    "up-bank",
    "banking",
    "api",
    "claude",
    "ai",
    "openapi",
    "typescript"
  ],
  "homepage": "https://github.com/wakeless/up-mcp#readme",
  "bugs": {
    "url": "https://github.com/wakeless/up-mcp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakeless/up-mcp.git"
  },
  "license": "MIT",
  "author": "Michael Gall",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "up-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "openapi.json"
  ],
  "scripts": {
    "generate:types": "openapi-typescript openapi.json -o src/generated/up-api.ts",
    "build": "npm run generate:types && tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^24.6.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1"
  }
}
